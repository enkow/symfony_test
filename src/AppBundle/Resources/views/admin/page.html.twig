{% extends 'AppBundle::admin/base.html.twig' %}

{% block stylesheets %}
	{{parent()}}
	{% block styles %}{% endblock %}
{% endblock %}

{% block page %}
	<section class="content">
		<div class="box">
			<div class="box-header with-border">
				<h3 class="box-title"> {% block title %}{% endblock %} </h3>
				<span class="pull-right"> {% block subtitle %}{% endblock %} </span>
			</div>
			<div class="box-body">
				{% include 'AppBundle::admin/flash_message.html.twig' %}
				{% block body %}{% endblock %}
			</div>
		</div>
	</section>
{% endblock %}

{% block javascripts %}
	{{parent()}}
	{% block scripts %}{% endblock %}
{% endblock %}
